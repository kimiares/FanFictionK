@model IEnumerable<FFK.Models.Category>

@{
    ViewData["Title"] = "GetAllCategories";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>GetAllCategories</h1>

<p>
    <a asp-action="CreateCategory" asp-controller="Category">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            
        </tr>
}
    </tbody>
</table>
