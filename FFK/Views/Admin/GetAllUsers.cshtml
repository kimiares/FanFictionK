@model IEnumerable<FFK.Models.User>


<div class="row border-dark">
    <div class="col-md-2">
        @await Html.PartialAsync("_AdminPageLayout")
    </div>
    <div div class="col-md-10">
        <table class="table">
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.Email</td>
                    <td>
                            <a class="btn btn-sm btn-primary" asp-action="EditRole" asp-controller="Admin" asp-route-userid="@user.Id">Rules</a>
                        </td>
                </tr>
            }
        </table>
        </div>
    </div>
